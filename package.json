{
  "name": "bundle-size",
  "version": "0.0.0",
  "description": "Compare bundles sizes of import vs. require",
  "main": "server.js",
  "scripts": {
    "build": "rm -rf .build && mkdir -p .build && npm run build-import-all && npm run build-import-separate && npm run build-require-all && npm run build-require-dynamic && npm run build-require-separate",
    "build-import-all": "webpack import-all.js -o .build/import-all.js",
    "build-import-separate": "webpack import-separate.js -o .build/import-separate.js",
    "build-require-all": "webpack require-all.js -o .build/require-all.js",
    "build-require-dynamic": "webpack require-dynamic.js -o .build/require-dynamic.js",
    "build-require-separate": "webpack require-separate.js -o .build/require-separate.js",
    "install": "npm run build"
  },
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bergos/bundle-size.git"
  },
  "bugs": {
    "url": "https://github.com/bergos/bundle-size/issues"
  },
  "homepage": "https://github.com/bergos/bundle-size",
  "dependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
